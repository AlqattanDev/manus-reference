---
import type { Education, EducationProps } from '@/types';

const { edu } = Astro.props as EducationProps;
---

<div class="entry">
  <div class="entry-header">
    <span class="entry-title">{edu.degree}</span>
    <span class="entry-meta">{edu.university}</span>
  </div>
  <p class="entry-description">Thesis: {edu.thesis}</p>
</div>
