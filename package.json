{
  "name": "ali-portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.{ts,astro}",
    "lint:fix": "eslint src/**/*.{ts,astro} --fix",
    "format": "prettier --write src/**/*.{ts,astro,css,json}",
    "format:check": "prettier --check src/**/*.{ts,astro,css,json}",
    "type-check": "astro check && tsc --noEmit"
  },
  "dependencies": {
    "astro": "^4.0.0",
    "hyper-mcp-shell": "^1.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.0",
    "@testing-library/dom": "^9.3.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.4",
    "happy-dom": "^12.10.3",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "typescript": "^5.0.0",
    "vitest": "^1.1.0"
  }
